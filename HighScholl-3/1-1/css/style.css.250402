* {font-style:normal; line-height:normal; font-family:"Pretendard", sans-serif; box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;}

/*
	font-family:"Inter", sans-serif;
	font-family:"ONE Mobile", sans-serif;
	font-family:"ONE-Mobile-Title", sans-serif;
	font-family:"Pretendard", sans-serif;
	font-family:"SEBANG Gothic", sans-serif;
	font-family:"WAGURI", sans-serif;
*/

html {-webkit-text-size-adjust:none; -moz-text-size-adjust:none; -ms-text-size-adjust:none; -o-text-size-adjust:none;}
body {width:100%; margin:0 auto; padding:0; font-size:13px; line-height:normal; overflow-y:hidden;}
img {border:none; margin:0; padding:0; outline:none;}
a {border:none; margin:0; padding:0; outline:none; text-decoration:none;}
a:active {color:#666666; text-decoration:none;}
a:link {color:#666666; text-decoration:none;}
a:visited {color:#666666; text-decoration:none;}
a:hover {color:#666666; text-decoration:none;}
p {margin:0; padding:0;}
ul {list-style:none; margin:0; padding:0;}
ul:after {content:""; display:block; clear:both;}
ul > li {display:block;}
button {border:0; background-color:transparent; margin:0; padding:0;}
h1, h2, h3, h4, h5, h6 {margin:0; padding:0;}

img.fx{max-width:100%;}

td {
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
}

input, select, textarea {
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
	margin:0; 
	padding:0; 
	outline:none; 
	font-size:20px; 
	color:#000000;
	line-height:140%; 
}

input::placeholder {color:#000000; opacity:1;}
input::-webkit-input-placeholder {color:#000000;}
input:-ms-input-placeholder {color:#000000;}
input:-moz-input-placeholder {color:#000000;}

textarea {overflow-y:scroll; resize:none;}

textarea::placeholder {color:#000000; opacity:1;}
textarea::-webkit-input-placeholder {color:#000000;}
textarea:-ms-input-placeholder {color:#000000;}
textarea:-moz-input-placeholder {color:#000000;}

input[type=text], input[type=password] ,input[type=tel], input[type=file], input[type=image], input[type=button], input[type=submit], textarea {
	border-radius:0; 
	-moz-border-radius:0; 
	-webkit-border-radius:0;
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	width:100%;
}

select {
	border-radius:0; 
	-moz-border-radius:0; 
	-webkit-border-radius:0;
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	background-color:#ffffff;
	background-image:url("../img/select_arrow.png");
	background-repeat:no-repeat;
	background-position:right 10px center;
	width:100%;
	padding:0 29px 0 10px;
}

select::-ms-expand {display:none;}

.w_con {}
.m_con {display:none;}

.w_img {display:inline-block; width:100%;}
.m_img {display:none;}

.w_br {}
.m_br {display:none;}

.w_bar {}
.m_bar {display:none;}

#wrap {width:100%; min-width:280px; margin:0 auto;}

video::cue {
	color:yellow; 
	background-color:#000; 
	background-color:rgba(0,0,0,0.5);
	font-size:18px;
	text-shadow:1px 1px 2px #000;
}

button, input[type=checkbox], input[type=radio] {cursor: pointer;}

.fw_500 {font-weight:500 !important;}
.fw_600 {font-weight:600 !important;}
.fw_700 {font-weight:700 !important;}

.f_center {display:flex; align-items: center; justify-content: center; text-align: center;}
 
caption {overflow:hidden; width:0; height:0; font-size:0; line-height:0;}
.blind {position: absolute; width: 1px; height: 1px; clip: rect(0 0 0 0); overflow: hidden;}

.both_wrap {display:flex; width:100%;}
.btn_data_popup {cursor:pointer; vertical-align:middle; margin-top:-8px; width:120px; height:43px; background:url(../img/data_popup_btn.png) no-repeat; background-size: contain;}
.btn_active_popup {cursor:pointer; vertical-align:middle;width:150px; height:58px; background:url(../img/active_popup_btn.png) no-repeat; background-size: contain;}
.link_tooltip {cursor:pointer; font-size:inherit; font-weight:700; color:#3C65FF !important; text-decoration:underline;}
.btn_link_popup {vertical-align:middle; width:39px; height:30px; background: url(../img/btn_link_popup.png) no-repeat; background-size: contain;}

button.after_hand::after {content:''; display:inline-block; width:65px; height:77px; background:url(../img/icon_hand.png) no-repeat; background-size:contain;}

.rd_10 {border-radius:10px;}
.rd_20 {border-radius:20px;}
.round_box {border-radius:20px; background-color:#F7F7F7;}

.number_icon {display:inline-block; margin-right:3px; color:#fff; vertical-align:top; width:31px; height:31px; font-size:22px; font-weight:700; text-align:center; line-height:140%; border-radius:50% 50% 0 50%; background:#00A799;}
.number_icon.type_red {background-color:#EF5E5E;}
.number_icon.type_blue {background-color:#4296FF;}
.number_icon.type_purple {background-color:#7A4CED;}

.input_select {width:211px; height:47px; padding:8px 20px; border-radius: 10px; border:1px solid #D9D9D9; font-size:22px; appearance: none; background:#fff url(../img/input_select.png) no-repeat center right 20px; background-size: 17px 10px;}

.data_list  {margin:0; display:flex; gap:11px; font-size:26px; margin-top:20px;}
.data_list dt {background-color: #4296FF;  text-align:center; justify-content: center;  color:#fff; font-weight:700; flex-basis: 300px; flex-shrink: 0; height:100px; display: flex; align-items: center;}
.data_list dd {margin:0; flex:1; background-color: #F6F6F6; padding:15px 72px; display: flex; align-items: center; text-align:center; justify-content: center; }

.data_list.type_exam {margin-top:0;}
.data_list.type_exam dt {background-color:#A1CBFF; color:#000; flex-basis: 168px; height:auto;}
.data_list.type_exam dd {text-align:left; padding:20px 30px; justify-content:flex-start ;}

/* title */
.sub_title_dot {color:#222; font-size:32px; font-weight:700; display:flex; gap:8px; align-items: center;}
.sub_title_dot:before {content:''; display:inline-block; width:15px; height:15px; border-radius: 50%; background-color:#4296FF;}

/* tab */
.tab_wrap {position:relative;}
.tab_wrap > .tab_list {display:flex; gap:20px; align-items: flex-start;}
.tab_wrap > .tab_list button { flex:1; color:#888; cursor:pointer; font-size:27px; font-weight:600; line-height:70px; height:90px; padding-bottom:20px; background-color:#ddd; border-radius:20px 20px 0 0;}
.tab_wrap > .tab_list button.on {font-size:30px; font-weight:700; background-color:#4296FF; color:#fff;}
.tab_wrap > .tab_contents {margin-top:-20px; position:relative; height:412px; border:4px solid #4296FF; border-radius:22px; background-color:#fff;}
.tab_wrap > .tab_contents .tab_inner {display:none; padding:30px 40px 0;}
.tab_wrap > .tab_contents .tab_inner.on {display:block;}

/* 팝업 */
.full_popup_wrap {display:none; position:absolute; top:0; left:0; width:100%; height:100%; background:rgba(0, 0, 0, 0.80); z-index:900;}
.full_popup {padding-top:22px; margin:146px 84px 0 79px; height:692px; position:relative; background:#fff; border-radius:10px; box-shadow: 0px 4px 20px 0px rgba(0, 0, 0, 0.25);}
.full_popup .btn_close {cursor:pointer; position:absolute; width:40px; height:40px; top:23px; right:20px; background:url(../img/full_popup_close_btn.png) no-repeat; background-size:contain;}
.full_popup > .title {margin-bottom:30px; color:#222; text-align:center; font-size:32px; font-weight:700;  line-height:41px;}
.full_popup > .full_popoup_content {padding:0 28px;}
.full_popup > .full_popoup_content .text { color:#222; font-size:28px; font-weight:500; line-height:39px;}

.exam_icon {display:inline-block; color:#888; font-weight:400; width:79px; height:40px; font-size:26px; text-align: center; line-height:36px; flex-shrink: 0; border-radius: 30px; border: 2px solid #888;}
.exam_txt {display:flex; gap:18px; color:#888; font-size:24px; font-weight:500; letter-spacing: -0.24px;}
.exam_txt > .txt {line-height:150%; flex:1; }
.exam_txt .exam_icon {flex-basis:79px;}
.exam_icon2 {display:inline-block; width:79px; height:40px; font-size:26px; line-height:40px; text-align:center; flex-shrink:0; flex-basis:80px; border-radius: 10px 10px 0px 10px; background: #666; color:#fff;}

.exam_textarea {padding:22px 18px; background: #F6F6F6; display:flex; flex-direction:column; height:190px;} 
.exam_textarea .exam_tit {flex-shrink:0; gap:18px; display:flex; color:#888; font-size:24px; font-weight:500; padding-bottom:14px; border-bottom:2px dashed #ddd;}
.exam_textarea .exam_tit .exam_icon {flex-shrink: 0;}
.exam_textarea .exam_tit p {flex:1; line-height:150%;}
.exam_textarea .exam_con {flex:1; }
.exam_textarea .exam_con textarea {height:100%; width:100%; font-size:26px; padding:20px 0; border:0; overflow:auto; background-color: transparent;}
.exam_textarea .exam_con textarea::placeholder {color:#ddd; text-align:center; line-height:75px;}

/* table */
.tab_con {border-top:5px solid #4296FF;}
.tab_con > table {width:100%; margin:0 auto; border-collapse: collapse; table-layout: fixed;}
.tab_con > table > thead > tr > td {border-bottom:6px solid #FFF; border-left:6px solid #FFF; background-color:#A1CBFF; padding:9px 0 11px 0; text-align:center;}
.tab_con > table > thead > tr > td span {font-size:26px; color:#000; font-weight:600;}
.tab_con > table > tbody > tr > td {border-top:6px solid #FFF; border-left:6px solid #FFF;}
.tab_con > table > tbody > tr > td span {font-size:26px; color:#000;}
.tab_con > table > tbody > tr > td:first-child {border-left:0;}
.tab_con > table > tbody > tr > .title_td {background:rgba(66,150,255,0.20); text-align:center;}
.tab_con > table > tbody > tr > .title_td span {font-weight:600;}
.tab_con > table > tbody > tr > .info_td {background:#F6F6F6; padding:15px 44px;}
.tab_con > table > tbody > tr > .info_td > ul {}
.tab_con > table > tbody > tr > .info_td > ul > li {}
.tab_con > table > tbody > tr > .info_td > ul > li span {display:inline-block; padding:0 0 0 24px; position:relative; top:0; left:0;}
.tab_con > table > tbody > tr > .info_td > ul > li span:before {content:''; width:5px; height:5px; background:#000; position:absolute; top:12.5px; left:0; border-radius:100%;}
.tab_con > table > thead > tr > td:first-child {border-left:0;}
.tab_con > table > thead > tr:first-child > td {border-top:0;}

/* text */
.text_01 {color:#191919; font-size:32px; line-height:143%; letter-spacing: -0.768px;}

/*텍스트 팝업*/
.text_popup_con {display:none; width:484px; border:3px solid #3C65FF; background-color:#FFF; position:absolute; top:0; left:0; z-index:100; border-radius:20px; box-shadow:0px 4px 8px 0px rgba(60,101,255,0.70);}
.text_popup_con > .contents_con {padding:15px 27px 21px 27px; position:relative; top:0; left:0;}
.text_popup_con > .contents_con > .close_btn {width:40px; position:absolute; top:17px; right:10px; z-index:10;}
.text_popup_con > .contents_con > .close_btn .a_btn {display:block; height:40px; background-image:url("../img/text_popup_close_btn.png"); background-repeat:no-repeat; background-size:40px; background-position:center;}
.text_popup_con > .contents_con > .title_con {margin:0 0 21px 0; text-align:center;}
.text_popup_con > .contents_con > .title_con span {font-size:32px; color:#3C65FF; font-weight:700; line-height:44.8px;}
.text_popup_con > .contents_con > .info_con {word-break:keep-all;}
.text_popup_con > .contents_con > .info_con span {font-size:27px; color:#1E1E1E;}
.text_popup_con > .contents_con > .info_con .list_item {display: list-item;}
 
/* 슬라이더 */
.slide_wrap_con {position:relative;}
.slide_wrap_con .slide_con .swiper-container {position:relative; top:0; left:0;}
.slide_wrap_con .slide_btn {display:block; width:50px; height:50px; opacity:1; border-radius:50%; margin-top:-25px;}
.slide_wrap_con .slide_btn.prev {left:0; background:#346DD0 url(../img/slider_prev_btn.png) no-repeat center; background-size: contain; }
.slide_wrap_con .slide_btn.next {right:0; background:#346DD0 url(../img/slider_next_btn.png) no-repeat center; background-size: contain;}
.slide_wrap_con .slide_btn.swiper-button-disabled,
.slide_wrap_con .slide_btn.swiper-button-disabled {background-color:#888 !important;}
.slide_wrap_con .swiper-container .swiper-pagination {position:static; width:100%; margin:24px auto 0; text-align:center;}
.slide_wrap_con .swiper-container .swiper-pagination .swiper-pagination-bullet {width:20px; height:20px; background-color:#888; margin:0 10px; border-radius:20px; opacity:1;}
.slide_wrap_con .swiper-container .swiper-pagination .swiper-pagination-bullet-active {width:40px; background-color:#222;}

/*H-3-01*/
.H_3_01_con {width:100%; max-width:1600px; margin:0 auto; background-image:url("../img/H_3_01_con_bg.png"); background-repeat:no-repeat; background-size:1600px; background-position:center;}
